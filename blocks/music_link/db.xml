<?xml version="1.0" encoding="UTF-8"?>
<schema
        xmlns="http://www.concrete5.org/doctrine-xml/0.5"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">

    <table name="btMusicLink">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="label" type="string" size="255" />

        <field name="spotify" type="string" size="255" />
        <field name="youtube" type="string" size="255" />
        <field name="soundcloud" type="string" size="255" />
        <field name="apple" type="string" size="255" />
        <field name="deezer" type="string" size="255" />
        <field name="tidal" type="string" size="255" />
        <field name="amazon" type="string" size="255" />
        <field name="napster" type="string" size="255" />
        <field name="itunes" type="string" size="255" />
    </table>
</schema>